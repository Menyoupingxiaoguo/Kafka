<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <add name="ConnectionString" connectionString="Data Source=dbhost;Initial Catalog=dbname;Persist Security Info=True;User ID=user;Password=pass" providerName="SQLSERVER" />
  </connectionStrings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!--Broker-->
    <add key="Broker" value="****:9092" />
    <!--Topic-->
    <add key="ConsumeTopic" value="" />
    <add key="ProduceTopic" value="" />
    <!--GroupID-->
    <add key="GroupID" value="" />
    <!--Partitions-->
    <add key="Partitions" value="0" />
    <!--ConsumerCount-->
    <add key="ConsumerCount" value="5" />
  </appSettings>
</configuration>